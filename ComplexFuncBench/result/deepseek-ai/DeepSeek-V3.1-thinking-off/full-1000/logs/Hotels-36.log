// Challenge 1: Student Grade Generator
function calculateGrade() {
    const marks = parseFloat(document.getElementById('marks').value);
    let grade;

    if (marks > 79) {
        grade = 'A';
    } else if (marks >= 60) {
        grade = 'B';
    } else if (marks >= 50) {
        grade = 'C';
    } else if (marks >= 40) {
        grade = 'D';
    } else {
        grade = 'E';
    }

    document.getElementById('gradeResult').innerText = `Grade: ${grade}`;
}

// Challenge 2: Speed Detector
function checkSpeed() {
    const speed = parseFloat(document.getElementById('speed').value);
    const speedLimit = 70;
    const kmPerDemeritPoint = 5;
    let demeritPoints = 0;

    if (speed <= speedLimit) {
        document.getElementById('speedResult').innerText = 'Ok';
    } else {
        demeritPoints = Math.floor((speed - speedLimit) / kmPerDemeritPoint);
        if (demeritPoints > 12) {
            document.getElementById('speedResult').innerText = 'License suspended';
        } else {
            document.getElementById('speedResult').innerText = `Points: ${demeritPoints}`;
        }
    }
}

// Challenge 3: Net Salary Calculator
function calculateNetSalary() {
    const basicSalary = parseFloat(document.getElementById('basicSalary').value);
    const benefits = parseFloat(document.getElementById('benefits').value);
    const grossSalary = basicSalary + benefits;

    // PAYE (Tax) Calculation
    let paye = 0;
    if (grossSalary <= 24000) {
        paye = grossSalary * 0.1;
    } else if (grossSalary <= 32333) {
        paye = 2400 + (grossSalary - 24000) * 0.25;
    } else {
        paye = 2400 + 2083.25 + (grossSalary - 32333) * 0.3;
    }

    // NHIF Deductions
    let nhif = 0;
    if (grossSalary <= 5999) {
        nhif = 150;
    } else if (grossSalary <= 7999) {
        nhif = 300;
    } else if (grossSalary <= 11999) {
        nhif = 400;
    } else if (grossSalary <= 14999) {
        nhif = 500;
    } else if (grossSalary <= 19999) {
        nhif = 600;
    } else if (grossSalary <= 24999) {
        nhif = 750;
    } else if (grossSalary <= 29999) {
        nhif = 850;
    } else if (grossSalary <= 34999) {
        nhif = 900;
    } else if (grossSalary <= 39999) {
        nhif = 950;
    } else if (grossSalary <= 44999) {
        nhif = 1000;
    } else if (grossSalary <= 49999) {
        nhif = 1100;
    } else if (grossSalary <= 59999) {
        nhif = 1200;
    } else if (grossSalary <= 69999) {
        nhif = 1300;
    } else if (grossSalary <= 79999) {
        nhif = 1400;
    } else if (grossSalary <= 89999) {
        nhif = 1500;
    } else if (grossSalary <= 99999) {
        nhif = 1600;
    } else {
        nhif = 1700;
    }

    // NSSF Deductions
    const nssf = grossSalary * 0.06; // Assuming Tier I

    const netSalary = grossSalary - paye - nhif - nssf;

    document.getElementById('netSalaryResult').innerHTML = `
        Gross Salary: ${grossSalary.toFixed(2)}<br>
        PAYE (Tax): ${paye.toFixed(2)}<br>
        NHIF Deductions: ${nhif.toFixed(2)}<br>
        NSSF Deductions: ${nssf.toFixed(2)}<br>
        Net Salary: ${netSalary.toFixed(2)}
    `;
}

