* {
    margin: 0;
    padding: 0;
    box-sizing: border-box;
}

body {
    font-family: Arial, Helvetica, sans-serif;
    line-height: 1.5;
    width: 60%;
    margin: 0 auto;
}

a {
    text-decoration: none;
}

.centered {
    text-align: center;
}

/* Header */
header {
    display: flex;
    flex-wrap: wrap;
    justify-content: space-between;
    margin: 25px 0;
}

.scores a,
.timer {
    font-size: 20px;
}

.scores a {
    color: purple;
    cursor: pointer;
}

.scores a:hover {
    color: rgba(232, 102, 236, 1);
}

/* Start Quiz */
.start-quiz {
    text-align: center;
}

h1 {
    margin-bottom: 10px;
}

h2 {
    color: grey;
    border-top: 1px solid grey;
    margin: 20px 0;
    font-style: italic;
    text-align: center;
}

/* Buttons */
.btn-grid {
    display: grid;
    grid-template-columns: repeat(2, auto);
    gap: 10px;
    margin: 20px 0;
}

button {
    font-family: Arial, Helvetica, sans-serif;
    border: none;
    width: auto;
    overflow: visible;
    padding: 10px;
    background-color: purple;
    border-radius: 10px;
    font-size: large;
    margin: 10px;
    color: white;
    cursor: pointer;
}

button:hover {
    background-color: rgba(232, 102, 236, 1);
}

.hide {
