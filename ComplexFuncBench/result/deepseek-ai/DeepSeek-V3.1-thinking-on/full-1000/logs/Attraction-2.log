```python
import json

def add_task(tasks, title, description):
    """Add a new task to the list of tasks."""
    task_id = len(tasks) + 1
    task = {
        'id': task_id,
        'title': title,
        'description': description,
        'status': 'todo'
    }
    tasks.append(task)
    return task

def remove_task(tasks, task_id):
    """Remove a task from the list of tasks."""
    for task in tasks:
        if task['id'] == task_id:
            tasks.remove(task)
            return True
    return False

def update_task(tasks, task_id, title, description):
    """Update a task in the list of tasks."""
    for task in tasks:
        if task['id'] == task_id:
            task['title'] = title
            task['description'] = description
            return True
    return False

def mark_task_as_completed(tasks, task_id):
    """Mark a task as completed."""
    for task in tasks:
        if task['id'] == task_id:
            task['status'] = 'done'
            return True
    return False

def filter_tasks_by_status(tasks, status):
    """Return a list of tasks with the specified status."""
    return [task for task in tasks if task['status'] == status]

def save_tasks_to_file(tasks, filename):
    """Save the list of tasks to a file."""
    with open(filename, 'w') as f:
        json.dump(tasks, f)

def load_tasks_from_file(filename):
    """Load the list of tasks from a file."""
    try:
        with open(filename, 'r') as f:
            return json.load(f)
    except FileNotFoundError:
        return []

def main():
    """Main function."""
    tasks = load_tasks_from_file('tasks.json')
    
    while True:
        print("\nTask Manager")
        print("1. Add a task")
        print("2. Remove a task")
        print("3. Update a task")
        print("4. Mark a task as completed")
        print("5. Filter tasks by status")
        print("6. Save tasks to a file")
        print("7. Load tasks from a file")
        print("8. Exit")
        
        choice = input("Enter your choice: ")
        
        if choice == '1':
            title = input("Enter the title of the task: ")
            description = input("Enter the description of the task: ")
            task = add_task(tasks, title, description)
            print(f"Task added with ID: {task['id']}")
            
        elif choice == '2':
            task_id = int(input("Enter the ID of the task to remove: "))
            if remove_task(tasks, task_id):
                print("Task removed successfully.")
            else:
                print("Task not found.")
                
        elif choice == '3':
            task_id = int(input("Enter the ID of the task to update: "))
            title = input("Enter the new title of the task: ")
            description = input("Enter the new description of the task: ")
            if update_task(tasks, task_id, title, description):
                print("Task updated successfully.")
            else:
                print("Task not found.")
                
        elif choice == '4':
            task_id = int(input("Enter the ID of the task to mark as completed: "))
            if mark_task_as_completed(tasks, task_id):
                print("Task marked as completed.")
            else:
                print("Task not found.")
                
        elif choice == '5':
            status = input("Enter the status to filter by (todo/done): ")
            filtered_tasks = filter_tasks_by_status(tasks, status)
            for task in filtered_tasks:
                print(f"ID: {task['id']}, Title: {task['title']}, Description: {task['description']}, Status: {task['status']}")
                
        elif choice == '6':
            save_tasks_to_file(tasks, 'tasks.json')
            print("Tasks saved to file.")
            
        elif choice == '7':
            tasks = load_tasks_from_file('tasks.json')
            print("Tasks loaded from file.")
            
        elif choice == '8':
            break
            
        else:
            print("Invalid choice. Please try again.")

if __name__ == '__main__':
    main()
